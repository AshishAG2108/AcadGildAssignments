<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.acadgild.Entity.Employee" table="Employee" catalog="employeeManagement">
        <id name="employeeId" type="java.lang.Integer">
            <column name="Empid" />
            <generator class="identity" />
        </id>
        <property name="empName" type="string">
            <column name="EmpName" length="200"  />
        </property>
        <property name="empSalary" type="int">
            <column name="EmpSalary"   />
        </property>
        
         <property name="empLocation" type="string">
            <column name="EmpLocatio" length="200"  />
        </property>
        
    </class>
</hibernate-mapping>