<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.acadgild.Entity.Floors" table="floors" catalog="employeeManagement">
        <id name="floorNo" type="java.lang.Integer">
            <column name="FloorNo" />
            <generator class="identity" />
        </id>
        <property name="seatNo" type="int">
            <column name="seatNo"   />
        </property>
        <property name="seatStatus" type="string">
            <column name="seatStatus"   />
        </property>
        
    </class>
</hibernate-mapping>